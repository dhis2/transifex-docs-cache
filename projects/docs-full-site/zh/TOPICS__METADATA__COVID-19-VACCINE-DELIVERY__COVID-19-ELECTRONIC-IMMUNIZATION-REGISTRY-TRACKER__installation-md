        "DHIS2Version": "2.36.6",
        "code": "CVIRAG",
        "description": "",
        "lastUpdated": "20220119T084447",
        "locale": "en",
        "name": "CVIR_EIR_AGG_1.2.0_DHIS2.36.6-en",
